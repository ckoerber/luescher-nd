  3.771912165377488*^9}},ExpressionUUID->"ec48b7dc-ee61-4f9a-a956-\
c52164d291b0"],

Cell[BoxData[
 RowBox[{"With", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"d", "=", "2"}], ",", 
     RowBox[{"plotter", "=", "ContourPlot"}], ",", 
     RowBox[{"nn", "=", "8."}], ",", " ", 
     RowBox[{"nss", "=", 
