       "}"}]}]}], "}"}], ",", "\[IndentingNewLine]", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"plotter", "@@", 
      RowBox[{"List", "[", 
       RowBox[{
        RowBox[{"Evaluate", "@", 
         RowBox[{"Sum", "[", 
          RowBox[{
           RowBox[{
            SuperscriptBox["L", "2"], 
            RowBox[{
             RowBox[{"\[Omega]", "[", "ns", "]"}], "[", 
             RowBox[{
              RowBox[{"2", " ", "\[Pi]", " ", 
               RowBox[{
                RowBox[{"n", "[", "dim", "]"}], "/", "L"}]}], " ", ",", 
              RowBox[{"L", "/", "nn"}]}], "]"}]}], ",", 
           RowBox[{"{", 
            RowBox[{"dim", ",", "1", ",", "d"}], "}"}]}], "]"}]}], ",", 
        RowBox[{"Sequence", "@@", 
